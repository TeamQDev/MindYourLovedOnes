                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="The ability to get on and off the toilet"
                    android:layout_below="@+id/txtT"
                    android:layout_alignParentLeft="true"
                    android:textSize="@dimen/dimension16dp"

                    />
                <ToggleButton
                    android:id="@+id/tbToileting"
                    android:layout_width="@dimen/dimension50dp"
                    android:layout_height="@dimen/dimension50dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dimension15dp"
                    android:background="@drawable/toggle_selector"
                    android:checked="false"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rlTranfering"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlToileting"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="Tranfering"
                    android:layout_toLeftOf="@+id/tbTranfering"
                    android:layout_alignParentLeft="true"
                    android:id="@+id/txtDf"
                    android:textColor="@color/colorBlue"
                    />
