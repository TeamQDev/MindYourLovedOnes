<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:custom="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:custom="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/txtName"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dimension45dp"
            android:background="@color/colorSkyBlue"
            android:gravity="center"
            android:text="XYZ XYZ"
            android:textColor="@color/colorBlack"
            android:textSize="@dimen/dimension22dp" />
        <RelativeLayout
            android:id="@+id/rlFunctional"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtName"
            android:layout_margin="@dimen/dimension10dp"
            android:background="@drawable/gray_white_border"
            android:padding="@dimen/dimension5dp">

            <ImageView
                android:id="@+id/imgFunctional"
                android:layout_width="@dimen/dimension70dp"
                android:layout_height="@dimen/dimension70dp"

                android:layout_marginRight="@dimen/dimension10dp"
                android:padding="@dimen/dimension6dp"
                android:src="@drawable/functional"
                />

            <TextView
                android:id="@+id/txtFunctionalHeader"
                style="@style/EmergencyCategory"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimension40dp"
                android:layout_toRightOf="@+id/imgFunctional"
                android:text="Activities of Daily Living (ADL)" />
            <RelativeLayout
                android:id="@+id/rlBathing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/txtFunctionalHeader"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView
                    android:id="@+id/txtBathing"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="Bathing"
                    android:layout_toLeftOf="@+id/tbBathing"
                    android:layout_alignParentLeft="true"
                    android:textColor="@color/colorBlue"
                    />
                <TextView
                    style="@style/EmergencyDescs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="The ability to clean oneself and perform grooming activities like shaving and brushing teeth"
                    android:layout_below="@+id/txtBathing"
                    android:layout_alignParentLeft="true"
                    android:textSize="@dimen/dimension16dp"
                    />
                <ToggleButton
                    android:id="@+id/tbBathing"
                    android:layout_width="@dimen/dimension50dp"
                    android:layout_height="@dimen/dimension50dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dimension15dp"
                    android:background="@drawable/toggle_selector"
                    android:checked="false"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlContinence"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlBathing"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView
                    android:id="@+id/txtC"
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="Continence"
                    android:layout_toLeftOf="@+id/tbContinence"
                    android:layout_alignParentLeft="true"
                    android:textColor="@color/colorBlue"
                    />
                <TextView
                    style="@style/EmergencyDescs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="The ability to control one's bladder and bowel functions."
                    android:layout_below="@+id/txtC"
                    android:layout_alignParentLeft="true"
                    android:textSize="@dimen/dimension16dp"

                    />
                <ToggleButton
                    android:id="@+id/tbContinence"
                    android:layout_width="@dimen/dimension50dp"
                    android:layout_height="@dimen/dimension50dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dimension15dp"
                    android:background="@drawable/toggle_selector"
                    android:checked="false"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlDressing"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlContinence"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView

                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="Dressing"
                    android:layout_toLeftOf="@+id/tbDressing"
                    android:layout_alignParentLeft="true"
                    android:id="@+id/txtD"
                    android:textColor="@color/colorBlue"
                    />
                <TextView
                    style="@style/EmergencyDescs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="The ability to get dressed by oneself without struggling with buttons and zippers"
                    android:layout_below="@+id/txtD"
                    android:layout_alignParentLeft="true"
                    android:textSize="@dimen/dimension16dp"

                    />
                <ToggleButton
                    android:id="@+id/tbDressing"
                    android:layout_width="@dimen/dimension50dp"
                    android:layout_height="@dimen/dimension50dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dimension15dp"
                    android:background="@drawable/toggle_selector"
                    android:checked="false"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlFeeding"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlDressing"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:layout_alignParentLeft="true"
                    android:text="Eating"
                    android:layout_toLeftOf="@+id/tbfeed"
                    android:id="@+id/txtF"
                    android:textColor="@color/colorBlue"
                    />
                <TextView
                    style="@style/EmergencyDescs"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="The ability to feed oneself"
                    android:layout_below="@+id/txtF"
                    android:layout_alignParentLeft="true"
                    android:textSize="@dimen/dimension16dp"

                    />
                <ToggleButton
                    android:id="@+id/tbfeed"
                    android:layout_width="@dimen/dimension50dp"
                    android:layout_height="@dimen/dimension50dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dimension15dp"
                    android:background="@drawable/toggle_selector"
                    android:checked="false"
                    android:gravity="center_vertical"
                    android:text=""
                    android:textOff=""
                    android:textOn="" />
            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rlToileting"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlFeeding"
                android:layout_toRightOf="@+id/imgFunctional"
               android:layout_marginTop="@dimen/dimension10dp"
                >

                <TextView
                    style="@style/EmergencyDesc"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimension50dp"
                    android:gravity="center_vertical"
                    android:padding="@dimen/dimension5dp"
                    android:text="Toileting"
                    android:layout_toLeftOf="@+id/tbToileting"
                    android:layout_alignParentLeft="true"
                    android:id="@+id/txtT"
                    android:textColor="@color/colorBlue"
                    />
                <TextView
